{
  "tags": {
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "jsdoc-route-plugin"],
  "opts": {
    "template": "node_modules/ink-docstrap/template/",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "systemName": "Re-design Documentation",
    "theme": "cerulean"
  }
}
